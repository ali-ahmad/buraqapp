<div class="single-pro-review-area mt-t-30 mg-b-15">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="product-payment-inner-st">
          <ul id="myTabedu1" class="tab-review-design">
            <li class="active"><a href="#description">Courses Details</a></li>
            <li><a href="#reviews"> Acount Information</a></li>
            <li><a href="#INFORMATION">Social Information</a></li>
          </ul>


        <%= form_for(@user, url: users_path, method: 'POST', :html => {:class => 'needsclick addcourse', id:'demo1-upload' }) do |f| %>
          <div id="myTabContent" class="tab-content custom-product-edit">
            <div class="product-tab-list tab-pane fade active in" id="description">
              <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                  <div class="review-content-section">
                    <div class="dropzone dropzone-custom needsclick addcourse" id="demo1-upload">
                      <div id="dropzone1" class="pro-ad addcoursepro">
                        <div class="form-group alert-up-pd">
                          <div class="dz-message needsclick download-custom">
                            <i class="fa fa-download edudropnone" aria-hidden="true"></i>
                            <h2 class="edudropnone">Drop image here or click to upload.</h2>
                            <p class="edudropnone"><span class="note needsclick">(This is just a demo dropzone. Selected image is <strong>not</strong> actually uploaded.)</span>
                            </p>
                            <%= f.file_field :large_photo, :class=>'hd-pro-img' %>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-lg-6 col-md-6 col-xs-12">
                       <div class="form-group alert-up-pd">
                          <div class="dz-message needsclick download-custom">
                            <i class="fa fa-download edudropnone" aria-hidden="true"></i>
                            <h2 class="edudropnone">Drop image here or click to upload.</h2>
                            <p class="edudropnone"><span class="note needsclick">(This is just a demo dropzone. Selected image is <strong>not</strong> actually uploaded.)</span>
                            </p>
                            <%= f.file_field :photo, :class=>'hd-pro-img' %>
                          </div>
                        </div>
                      </div>
                      <div class="col-lg-6 col-md-6 col-xs-12">
                          <div class="form-group">
                            <%= f.text_field :firstname, id:'Firstname', :class =>"form-control", placeholder:'First Name' %>
                          </div>
                          <div class="form-group">
                            <%= f.text_field :lastname, id:'Lastname', :class =>"form-control", placeholder:'Last Name' %>
                          </div>
                          <div class="form-group">
                            <%= f.text_field :my_skills, id:'SkillName', :class =>"form-control", placeholder:'Skill Name' %>
                          </div>
                          <div class="form-group">
                            <%= f.text_field :special_designation, id:'Specialdesignation', :class =>"form-control", placeholder:'Special Designation' %>
                          </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-md-6 col-xs-12">
                        <div class="form-group">
                          <div class="input-mark-inner mg-b-22">
                            <%= f.text_field :phone, :class =>"form-control", datamask: "(999) 999-9999",  placeholder:'(999) 999-9999' %>
                            <span class="help-block"></span>                                  
                          </div>
                        </div>
                      </div>
                      <div class="col-md-6 col-xs-12">
                        <div class="form-group">
                          <%= f.telephone_field :phone, id:'fphone', :class =>"form-control", placeholder:'Primary Phone' %>
                        </div>
                      </div>
                      <div class="col-md-6 col-xs-12">
                        <div class="form-group">
                          <%= f.select(:gender, [ 'Not interested', 'Male', 'Female' ], {}, { :class => 'form-control' }) %>
                        </div>
                      </div>
                      <div class="col-md-6 col-xs-12">
                        <div class="form-group data-custon-pick" id="data_1">
                          <div class="input-group date">
                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                            <%= f.text_field :datetime,  :class =>"form-control", value:'10/04/2017' %>

                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-lg-12">
                        <div class="payment-adress">
                        <!--   <button type="submit" class="btn btn-primary waves-effect waves-light">Submit</button> -->
                          <a href="#" class="btn btn-primary waves-effect waves-light">Next</a>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="product-tab-list tab-pane fade" id="reviews">
              <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                  <div class="review-content-section">
                    <div class="row">
                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="row">
                          <div class="col-md-6 col-xs-12">
                            <div class="form-group">
                              <%= f.text_field :address, id:'Address', :class =>"form-control", placeholder:'Flat NO/House No' %>
                            </div>     
                          </div>
                          <div class="col-md-6 col-xs-12 margin-bottom-none">
                            <div class="form-group">
                              <%= f.text_field :location, id:'LandMark', :class =>"form-control", placeholder:'Location Map' %>
                            </div>
                          </div>
                        </div>
                        <div class="row">
                          <div class="col-md-6 col-xs-12">
                            <div class="form-group">
                              <%= f.text_field :country, id:'country', :class =>"form-control", placeholder:'Country' %>
                            </div>   
                          </div>
                          <div class="col-md-6 col-xs-12">
                            <div class="form-group">
                              <%= f.text_field :province, id:'Province', :class =>"form-control", placeholder:'Province' %>
                            </div>
                          </div>
                          <div class="col-md-6 col-xs-12">
                            <div class="form-group">
                              <%= f.text_field :city, id:'City', :class =>"form-control", placeholder:'City/Town' %>
                              <label for="City"></label>
                            </div>    
                          </div>
                          <div class="col-md-6 col-xs-12">
                            <div class="form-group">
                              <%= f.text_field :area, id:'Area_Name', :class =>"form-control", placeholder:'Area Name' %>  
                            </div>     
                          </div>   
                        </div>
                        <div class="devit-card-custom">
                          <div class="form-group">
                            <input type="text" class="form-control" placeholder="Email">
                          </div>
                          <div class="form-group">
                            <input type="number" class="form-control" placeholder="Phone">
                          </div>
                          <div class="form-group">
                            <input type="password" class="form-control" placeholder="Password">
                          </div>
                          <div class="form-group">
                            <input type="password" class="form-control" placeholder="Confirm Password">
                          </div>
                          <div class="row">
                            <div class="col-lg-12">
                              <div class="payment-adress">
                              <!--   <button type="submit" class="btn btn-primary waves-effect waves-light">Submit</button> -->
                                <a href="#" class="btn btn-primary waves-effect waves-light">Next</a>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="product-tab-list tab-pane fade" id="INFORMATION">
              <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                  <div class="review-content-section">
                    <div class="row">
                      <div class="col-lg-12">
                        <div class="devit-card-custom">
                          <div class="row">  
                            <div class="col-md-6 col-xs-12">
                              <div class="form-group">
                                <%= f.email_field :email, id:'Email1', :class =>"form-control", placeholder:'Registered Email' %>
                              </div>
                            </div>
                            <div class="col-md-6 col-xs-12">
                              <div class="form-group">
                                <%= f.email_field :email2, id:'Email2', :class =>"form-control", placeholder:'Alternate email' %>
                              </div>
                            </div>
                            <div class="col-md-6 col-xs-12">
                              <div class="form-group"> 
                                <%= f.text_field :new_albem, id:'NewAlebam', :class =>"form-control", placeholder:'New Alebam Url' %>
                              </div> 
                            </div>
                            <div class="col-md-6 col-xs-12">
                              <div class="form-group">  
                                <%= f.text_field :website, id:'Websitesurl', :class =>"form-control", placeholder:'Websites Url' %>
                              </div> 
                            </div>
                            <div class="col-md-6 col-xs-12">
                              <div class="form-group">
                                <%= f.text_field :facebook_link, id:'Facebook Address', :class =>"form-control", placeholder:'Facebook URL' %>  
                              </div>
                            </div>
                            <div class="col-md-6 col-xs-12">
                              <div class="form-group">
                                <%= f.text_field :twitter_link, id:'Twitter_Account', :class =>"form-control", placeholder:'Twitter URL' %>
                              </div>
                            </div>
                            <div class="col-md-6 col-xs-12">
                              <div class="form-group">
                                <%= f.text_field :whatsapp, id:'WhatsApp_no', :class =>"form-control", placeholder:'WhatsApp No' %>
                              </div>
                            </div>
                            <div class="col-md-6 col-xs-12">
                              <div class="form-group">
                                <input type="url" class="form-control" placeholder="Google Plus">
                              </div>
                            </div>

                            <div class="col-md-6 col-xs-12">
                              <div class="form-group">
                                <input type="url" class="form-control" placeholder="Linkedin URL">
                              </div>
                            </div>
                          </div>
                          <button type="submit" class="btn btn-primary waves-effect waves-light">Submit</button>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div
          <% end %>
        </div>


        
      </div>
    </div>
  </div>
</div>





